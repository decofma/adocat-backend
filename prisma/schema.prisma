// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @unique @default (cuid())
  name String
  email String
  birthdate DateTime
  createdAt DateTime @default(now())
}

model Cat {
  id String @id @unique @default (cuid())
  name String
  age String
  gender String
  race String
  castrated String
  vaccines String
  email String
  phone String
  owner String
  createdAt DateTime @default(now())
}